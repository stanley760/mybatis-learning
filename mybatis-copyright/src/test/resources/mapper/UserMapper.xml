<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.apache.ibatis.copyright.dao.IUserDao">

    <select id="queryUserInfoById" parameterType="int" resultType="org.apache.ibatis.copyright.entity.User">
        select id, user_name, age from user where id = #{id}
    </select>

</mapper>